<%= form_with url: searches_search_path, method: :get, local: :true do |f| %>
  <%= f.select :range, options_for_select([["User"], ["Book"]]) %>
  <%= f.select :match, options_for_select([["完全一致", "perfect_match"], ["部分一致", "partial_match"]]) %>
  <%= f.text_field :word %>
  <%= f.submit %>
  <% end %>